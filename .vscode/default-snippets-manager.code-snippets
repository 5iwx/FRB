{
	"冯如杯--插入图片": {
		"prefix": "冯如杯--插入图片",
		"description": "冯如杯--插入图片",
		"scope": "latex",
		"body": [
			"%插入一张图片\r",
			"\\begin{figure}[H] %H为当前位置，!htb为忽略美学标准，htbp为浮动图形\r",
			"    \\centering %图片居中\r",
			"    \\includegraphics[width=0.8\\textwidth]{example-image-2.png} %插入图片，[]中设置图片大小，{}中是图片文件名\r",
			"    \\caption{example\\_caption} %最终文档中希望显示的图片标题\r",
			"    \\label{example_label} %用于文内引用的标签\r",
			"\\end{figure}"
		]
	},
	"1*3子图示意图结构": {
		"prefix": "1*3子图示意图结构",
