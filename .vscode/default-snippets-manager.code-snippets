		"scope": "latex",
		"body": [
			"% 一行三张子图并排示意\r",
			"\\begin{figure}[htbp]\r",
			"  \\begin{subfigure}{0.31\\textwidth}\r",
			"    \\includegraphics[width=\\linewidth]{example-image-1.png}\r",
			"    \\caption{示意图1} \\label{fig:9aaa}\r",
			"  \\end{subfigure}%\r",
			"  \\hspace*{\\fill}   % maximize separation between subfigures\r",
			"  \\begin{subfigure}{0.31\\textwidth}\r",
			"    \\includegraphics[width=\\linewidth]{example-image-1.png}\r",
			"    \\caption{示意图2} \\label{fig:9bbb}\r",
			"  \\end{subfigure}\r",
			"  \\hspace*{\\fill}   % maximizeseparation between subfigures\r",
			"  \\begin{subfigure}{0.31\\textwidth}\r",
			"    \\includegraphics[width=\\linewidth]{example-image-1.png}\r",
			"    \\caption{示意图3} \\label{fig:9ccc}\r",
			"    \\end{subfigure}\r",
			"\\caption{一行三张子图并排示意}\r",
			"\\label{qmix-train}\r",
			"\\end{figure}"
